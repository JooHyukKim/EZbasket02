{
  "manifest_version": 2,

  "name": "ez_basket_chromeExtension",
  "description": "easily add favorite product and compare price",
  "version": "2.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
  	"contextMenus",
    "tabs",
    "<all_urls>",
    "storage",
    "activeTab",
    "declarativeContent"
  ]
  
}